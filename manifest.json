{
  "manifest_version": 2,
  "name": "KineticU",
  "version": "0.1",
  "background": {
    "scripts": ["scripts/background.js"]
  },
  "content_security_policy": "script-src 'self' https://cdnjs.cloudflare.com https://cdn.jsdelivr.net; object-src 'self'",
  "chrome_url_overrides": {
    "newtab": "mich_gecko.html"
  },
  "browser_action": {
    "default_icon": {
      "32": "Images/icon.png"
    }
  },
  "permissions": [
    "tabs"
  ]
}
